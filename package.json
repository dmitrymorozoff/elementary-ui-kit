{
  "name": "elementary-ui-kit",
  "version": "0.0.5",
  "description": "A collection of reusable React components built with styled-components.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitrymorozoff/elementary-ui-kit"
  },
  "scripts": {
    "start": "parcel src/docs/index.html",
    "docs": "styleguidist server",
    "build": "npm run clean && webpack --mode=production && npm run prepublish",
    "build:docs": "styleguidist build",
    "prepublish": "cpx ./README.md build && cpx ./package.json build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf build/*"
  },
  "author": "Morozov Dmitry",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cpx": "^1.5.0",
    "eslint": "^5.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "^7.10.0",
    "parcel-bundler": "^1.9.3",
    "path": "^0.12.7",
    "react-styleguidist": "^7.0.19",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "polished": "^1.9.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.3"
  },
  "babel": {
    "plugins": [
      "babel-plugin-styled-components"
    ]
  }
}
